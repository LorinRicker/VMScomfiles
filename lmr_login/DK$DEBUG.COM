DEFINE/KEY/NOLOG /TERMINATE /ECHO e4 "DISPLAY REG AT (6,11,42,38)"
DEFINE/KEY/NOLOG /TERMINATE /ECHO e5 "DISPLAY /PUSH REG"
DEFINE/KEY/NOLOG /TERMINATE /ECHO e6 "DISPLAY /POP REG"
DEFINE/KEY/NOLOG /TERMINATE /ECHO /IF_STATE=GOLD kp9 "DISPLAY SRC, REG, OUT"
DEFINE/KEY/NOLOG /TERMINATE /ECHO /IF_STATE=BLUE kp9 "DISPLAY INST, REG, OUT"
DEFINE/KEY/NOLOG /TERMINATE /ECHO /IF_STATE=GOLD minus "@DBG$DISPLAY"
DEFINE/KEY/NOLOG /NOSET_STATE/TERMINATE /ECHO pf2 "SPAWN TYPE lmr$login:dk$debug_keypad.gold"
DEFINE/KEY/NOLOG /NOSET_STATE /IF_STATE=GOLD/TERMINATE /ECHO pf2 "SPAWN TYPE lmr$login:dk$debug_keypad.gold"
DEFINE/KEY/NOLOG /NOSET_STATE /IF_STATE=BLUE/TERMINATE /ECHO pf2 "SPAWN TYPE lmr$login:dk$debug_keypad.blue"
